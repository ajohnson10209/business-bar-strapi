{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "name": "Client"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Email": {
      "type": "email",
      "required": false
    },
    "Last_Login": {
      "type": "datetime"
    },
    "Last_Name": {
      "type": "string"
    },
    "profile": {
      "model": "cp-profile",
      "via": "client"
    },
    "First_Name": {
      "type": "string"
    },
    "Password": {
      "type": "string"
    },
    "IsAdmin": {
      "type": "boolean",
      "default": false
    },
    "questionnaire_answer": {
      "model": "questionnaire-answers",
      "via": "client"
    },
    "invoice_items": {
      "collection": "invoice-items",
      "via": "client"
    },
    "payment_items": {
      "collection": "payment-items",
      "via": "client"
    },
    "contract": {
      "model": "contract",
      "via": "client"
    },
    "appointments": {
      "collection": "appointment",
      "via": "client"
    },
    "stripe_customer_id": {
      "type": "string",
      "required": true
    }
  }
}
